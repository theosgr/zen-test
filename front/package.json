{
  "name": "zen-front",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "autoprefixer": "^9.0.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "core-js": "^2.6.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^7.0.0",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^23.4.2",
    "lodash": "^4.17.10",
    "material-ui-pickers": "^1.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.2.1",
    "postcss-safe-parser": "^4.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^2.1.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.6.3",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.1",
    "react-number-format": "^4.0.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "resolve": "^1.8.1",
    "store2": "^2.7.0",
    "style-loader": "^0.21.0",
    "styled-components": "^4.1.3",
    "superagent": "^3.8.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.28.1",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^2.0.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "node scripts/test.js --env=jsdom --watch",
    "analyze": "source-map-explorer build/static/js/main.*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "require-context-hook"
        ]
      }
    }
  },
  "browserslist": [
    "> 0.2% in fr"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-knobs": "^4.1.1",
    "@storybook/addon-links": "^4.1.1",
    "@storybook/addon-storyshots": "^4.1.1",
    "@storybook/addons": "^4.1.1",
    "@storybook/react": "^4.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint-config-react-app": "^3.0.5",
    "react-test-renderer": "^16.6.3",
    "source-map-explorer": "^1.5.0",
    "storybook-host": "^5.0.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
